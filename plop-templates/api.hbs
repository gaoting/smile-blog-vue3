import request from '@/utils/request' //封装的axios

//name为plop命令行传过来的参数 拼接以区分不同页面模块 避免一个页面引入同多个同类型接口的时候冲突
export function get{{name}}List(params) {
  return request.get('/api/v1/{{api}}', params)
}

export function create{{name}}(params) {
  return request.post('/api/v1/{{api}}', params)
}

export function edit{{name}}(id) {
  return request.get('/api/v1/{{api}}/' + id + '/edit')
}

export function update{{name}}(id,params) {
  return request.put('/api/v1/{{api}}/' + id, params)
}

export function delete{{name}}(id) {
  return request.delete('/api/v1/{{api}}/' + id)
}